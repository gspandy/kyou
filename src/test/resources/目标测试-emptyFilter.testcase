[[DOC]]
<?xml version="1.0"?>
<document>
    <field name="aaa">a</field>
    <field name="bbb">b</field>
    <struct name="sss">
    	<field name="s1">111</field>
    	<field name="s2">222</field>
    </struct>
    <struct name="eee">
    </struct> 
</document>


[[STYLE]]
<?xml version="1.0"?>
<pack>
    <config>
        <encoding>utf8</encoding>
    </config>

    <style match="document">
        <format>{%%}</format>
        <param>m</param>
        <param>bk 1</param>
    </style>

    <style match="field">
        <format>"%":"%",</format>
        <param>s2b n</param>
        <param>s2b v</param>
    </style>
    
    <style match="struct:empty">
    	<format>ASDFASDFASDF</format>
    </style>

    <style match="struct">
    	<format>"%":{%%},</format>
    	<param>s2b n</param>
    	<param>m</param>
    	<param>bk 1</param>
    </style>
</pack>



[[EXPECTED]]
{"aaa":"a","bbb":"b","sss":{"s1":"111","s2":"222"},ASDFASDFASD}