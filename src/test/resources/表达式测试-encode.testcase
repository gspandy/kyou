[[STYLE]]
<?xml version="1.0"?>
<pack>
    <config>
        <encoding>utf8</encoding>
    </config>

    <style match="document">
        <format>%</format>
        <param>m</param>
    </style>

    <style match="struct">
        <format>%\r\n</format>
        <param>m</param>
    </style>

    <style match="#url field">
        <format>%</format>
        <param>s2b lowercases urlencode v</param>
    </style>
    <style match="#url_gbk field">
        <format>%</format>
        <param>s2b lowercases urlencode.gbk v</param>
    </style>
    <style match="#xml field">
        <format>%</format>
        <param>s2b xmlencode v</param>
    </style>
    <style match="#json field">
        <format>%</format>
        <param>s2b jsonencode v</param>
    </style>
    
    <style match="struct field">
    	<format></format>
   	</style>
</pack>


